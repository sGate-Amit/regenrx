                  <h1 class="uk-article-title">Privacy Policy</h1>
                  <div class="uk-margin-medium" property="text">
                     <div data-elementor-type="wp-page" data-elementor-id="3" class="elementor elementor-3">
                        <section class="elementor-section elementor-top-section elementor-element elementor-element-14dd7400 elementor-section-boxed elementor-section-height-default elementor-section-height-default" data-id="14dd7400" data-element_type="section">
